SELECT Email, PATINDEX('%@a.com', Email) AS FirstOccurence FROM [dbo].[tblPerson];

SELECT Email, PATINDEX('%@a.com', Email) AS FirstOccurence FROM [dbo].[tblPerson]
WHERE PATINDEX('%a.com', Email) > 0;

SELECT Email, REPLACE(Email, '.com','.in') AS ConvertedEmail FROM [dbo].[tblPerson];
